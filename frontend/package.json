    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.7",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint-config-next": "^13.3.2",
