    "@reduxjs/toolkit": "^1.8.5",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "18.11.9",
    "@types/ramda": "^0.28.20",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@use-gesture/react": "^10.2.23",
    "axios": "^1.3.5",
    "dayjs": "^1.11.5",
    "next": "^13.4.2-canary.4",
    "postcss": "^8.4.21",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-markdown": "^8.0.7",
    "react-redux": "^8.0.5",
    "react-table": "^7.8.0",
    "react-transition-group": "^4.4.5",
    "tailwindcss": "^3.2.4",
    "transition-component": "^1.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@storybook/addon-actions": "^7.0.0",
    "@storybook/addon-controls": "^7.0.0-rc.8",
    "@storybook/addon-essentials": "^7.0.0-rc.8",
    "@storybook/addon-interactions": "^7.0.0-rc.8",
    "@storybook/addon-links": "^7.0.0-rc.8",
    "@storybook/blocks": "^7.0.0-rc.8",
