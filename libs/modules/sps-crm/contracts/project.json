{
  "name": "@sps/sps-crm-contracts",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/modules/sps-crm/contracts/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/modules/sps-crm/contracts",
        "main": "libs/modules/sps-crm/contracts/src/index.ts",
        "tsConfig": "libs/modules/sps-crm/contracts/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/modules/sps-crm/contracts/jest.config.ts"
      }
    }
  },
  "tags": []
}
