{
  "name": "@sps/buildable",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "lib/shared/buildable/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/lib/shared/buildable",
        "main": "lib/shared/buildable/src/index.ts",
        "tsConfig": "lib/shared/buildable/tsconfig.lib.json",
        "assets": ["lib/shared/buildable/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "lib/shared/buildable/jest.config.ts"
      }
    }
  },
  "tags": []
}
