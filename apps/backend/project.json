{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend",
  "projectType": "application",
  "targets": {
    "develop": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "cd apps/backend/ && ../../node_modules/@strapi/strapi/bin/strapi.js develop"
          }
        ]
      }
    },
    "debug": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "cd apps/backend/ && node --inspect=0.0.0.0:9230 ../../node_modules/@strapi/strapi/bin/strapi.js develop"
          }
        ]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cd apps/backend && node ../../node_modules/@strapi/strapi/bin/strapi.js build"
        ]
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cd apps/backend && node ../../node_modules/@strapi/strapi/bin/strapi.js start"
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "ignorePath": "apps/backend/.eslintignore",
        "quiet": true
      }
    }
  },
  "tags": []
}
